<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="sinder.cse40333.sinderapp">

	<application
		android:allowBackup="true"
		android:icon="@drawable/sinder_logo"
		android:label="@string/app_name"
		android:roundIcon="@drawable/sinder_logo"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<uses-permission android:name="android.permission.CAMERA" />
		<uses-feature android:name="android.hardware.camera" android:required="true" />
		<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
		<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
		<activity
			android:name=".LaunchActivity"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".Welcome"></activity>
		<activity android:name=".Welcome_Detail"></activity>
		<activity android:name=".Forms_V"></activity>
		<activity android:name=".SavedProjects_V"></activity>
		<activity android:name=".PastProjects_SM"></activity>
		<activity android:name=".AddProjects_SM"></activity>
		<activity android:name = ".LoginActivity"></activity>
		<activity android:name = ".Welcome"></activity>
		<activity android:name = ".BaseActivity"></activity>
	</application>

</manifest>